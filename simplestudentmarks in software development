<script>
    var welcome=alert("welcome to calculate your marks")
    var a;
    var b;
    var c;
    var d;
    var e;
    var f;
    var g;
    var h;
    var i;
    var j;
    var k;
    var l;
    var m;
    var n;
    var o;
    var p;
    var q;
    var r;
    
    
    var result=0;
    var avg=0;
    function marks(){
        a=parseFloat(form1.n1.value);
        b=parseFloat(form1.n2.value);
        c=parseFloat(form1.n3.value);
        d=parseFloat(form1.n4.value);
        e=parseFloat(form1.n5.value);
        f=parseFloat(form1.n6.value);
        g=parseFloat(form1.n7.value);
        h=parseFloat(form1.n8.value);
        i=parseFloat(form1.n9.value);
        j=parseFloat(form1.n10.value);
        k=parseFloat(form1.n11.value);
        l=parseFloat(form1.n12.value);
        m=parseFloat(form1.n13.value);
        n=parseFloat(form1.n14.value);
        o=parseFloat(form1.n15.value);
        p=parseFloat(form1.n16.value);
        q=parseFloat(form1.n17.value);
        r=parseFloat(form1.n18.value);
        result=a+b+c+d+e+f+g+h+i+j+k+l+m+n+o+p+q+r;
        avg=result/18
       

        document.getElementById("ibyambere").innerHTML=result;
        document.getElementById("ibyakabiri").innerHTML=avg ;
      
        if(avg>=70){
            document.getElementById("ibyakane").innerHTML="pass";
        }
        else{
            document.getElementById("ibyakane").innerHTML="fail"; 
        }
        
        if(result>=1200){
            document.getElementById("ibyagatatu").innerHTML="conglatration you won!!!";
        }
        else{
            document.getElementById("ibyagatatu").innerHTML="try to work hard please!!!";
        }

       
      
    };
  
    
    function reload(){
        location.reload()
    }
  
</script>
